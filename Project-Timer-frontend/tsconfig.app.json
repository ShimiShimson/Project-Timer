/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "src",
    "paths": {
      "@app-services/*": ["app/core/services/*"],
      "@app-interfaces/*": ["app/shared/interfaces/*"],
      "@app-add-project/*": ["app/dashboard/add-project/*"],
      "@app-dashboard/*": ["app/dashboard/*"],
      "@app-dashboard-projects/*": ["app/dashboard/projects/*"],
      "@app-dialog/*": ["app/dashboard/add-project/dialog/*"],
      "@app-login-page/*": ["app/login-page/*"],
      "@app-material/*": ["app/shared/material/*"],
      "@app-nav/*": ["app/dashboard/nav/*"],
      "@environments/*": ["environments/*"],
      "@home/*": ["app/home/*"],
    }
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.d.ts"]
}
